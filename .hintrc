{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 123",
    "not chrome <= 124",
    "not edge <= 123",
    "not firefox <= 125",
    "not safari <= 17.4",
    "not samsung <= 24"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-tap-highlight-color",
          "user-select"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "button-type": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ]
  }
}